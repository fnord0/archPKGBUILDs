post_install() {
    echo ""
    echo ">> NOTE: run '/usr/bin/tcshrc_config -s' to install all"
    echo ">> tcshrc files to /etc/skel folder for system-wide install."
    echo ""
    echo ">> NOTE: run '/usr/bin/tcshrc_config -u' for installation of"
    echo ">> all tcshrc files to the current user folder."
    echo ""
}

post_upgrade() {
	post_install $1
}
